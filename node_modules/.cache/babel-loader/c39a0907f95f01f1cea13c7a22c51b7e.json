nt":["(function() {\n    'use strict';\n    var YESTAE_GL = (window.YESTAE_GL = window.appGlobal =\n        window.appGlobal || {});\n\n    // 应用信息\n    YESTAE_GL.appInfo = {\n        version: '1.0.0'\n    };\n\n    // 登陆用户信息\n    YESTAE_GL.user = {};\n\n    // JsBridge对象\n    YESTAE_GL.JsBridge = null;\n\n    /**\n     * 获取浏览器参数值\n     * 若需要在Controller中使用，可直接使用$location.$$search.name获得\n     * @param  {String} name    参数名称\n     * @param  {String} s       待查询的链接\n     * @return {String}         参数值\n     */\n    YESTAE_GL.getQueryStringRegExp = function(name, s) {\n        var reg = new RegExp('(^|\\\\?|&)' + name + '=([^&]*)(\\\\s|&|$)', 'i');\n        var uri = '';\n        if (s) {\n            uri = decodeURIComponent(s);\n        } else {\n            uri = window.location.search;\n            if (!uri) {\n                uri = window.location.href;\n            }\n        }\n        if (reg.test(uri)) {\n            var result = decodeURIComponent(RegExp.$2.replace(/\\+/g, ' '));\n            return result === '' ? '' : result;\n        }\n        return '';\n    };\n\n    /**\n     * 获取浏览器所有参数值\n     */\n    YESTAE_GL.getRequest = function() {\n        var url = location.search; //获取url中\"?\"符后的字串\n        // var url = '?id=123&uid=456';\n        var theRequest = new Object();\n        if (url.indexOf('?') != -1) {\n            var str = url.substr(1);\n            strs = str.split('&');\n            for (var i = 0; i < strs.length; i++) {\n                theRequest[strs[i].split('=')[0]] = unescape(\n                    strs[i].split('=')[1]\n                );\n            }\n        }\n        return theRequest;\n    };\n\n    /**\n     * 是否微信浏览器\n     * @return {Boolean}\n     */\n    YESTAE_GL.isWeChatBrowser = function() {\n        var ua = window.navigator.userAgent.toLowerCase();\n        if (ua.match(/MicroMessenger/i) == 'micromessenger') {\n            return true;\n        } else {\n            return false;\n        }\n    };\n\n    /**\n     * 是否小程序环境\n     * @return {Boolean}\n     */\n    YESTAE_GL.isWxApp = function() {\n        return window.__wxjs_environment === 'miniprogram';\n    };\n\n    /**\n     * 是否Ios\n     * @return {Boolean}\n     */\n    YESTAE_GL.isIos = function() {\n        return /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;\n    };\n\n    /**\n     * 是否Android\n     * @return {Boolean}\n     */\n    YESTAE_GL.isAndroid = function() {\n        return (\n            navigator.userAgent.indexOf('Android') > -1 ||\n            navigator.userAgent.indexOf('Adr') > -1\n        );\n    };\n\n    /**\n     * 动态修改页面title\n     * @param  {String} title    title名称\n     */\n    YESTAE_GL.setTitle = function(title) {\n        document.title = title;\n        var i = document.createElement('iframe');\n        i.src = '';\n        i.style.display = 'none';\n        i.onload = function() {\n            setTimeout(function() {\n                i.remove();\n            }, 9);\n        };\n        document.body.appendChild(i);\n    };\n\n})();\n"]}]}